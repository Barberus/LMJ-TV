<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="SkinAddonListLayout">
		<param name="id" default="18000"/>
		<param name="left" default="15"/>
		<param name="top" default="150"/>
		<param name="width" default="1165"/>
		<param name="height" default="698"/>
		<param name="onleft" default=""/>
		<param name="visible" default="true"/>
		<param name="visible" default="true"/>
		<param name="itemlayouttextcolor1" default="grey3"/>
		<param name="itemlayouttextcolor2" default="grey3"/>
		<param name="focusedtextcolor1" default="black"/>
		<param name="focusedtextcolor2" default="black"/>
		<param name="itemgap" default="3"/>
		<definition>
			<control type="list" id="$PARAM[id]">
				<left>$PARAM[left]</left>
				<top>$PARAM[top]</top>
				<width>$PARAM[width]</width>
				<height>$PARAM[height]</height>
				<onup>$PARAM[id]</onup>
				<ondown>$PARAM[id]</ondown>
				<onleft>$PARAM[onleft]</onleft>
				<visible>$PARAM[visible]</visible>
				<itemgap>$PARAM[itemgap]</itemgap>
				<include>openclosevisible</include>
				<scrolltime tween="Quadratic" easing="out">500</scrolltime>
				<focusposition>0</focusposition>
				<orientation>vertical</orientation>
				<movement>1</movement>
				<itemlayout height="52" width="$PARAM[width]">
					<control type="label">
						<left>30</left>
						<top>15</top>
						<height>45</height>
						<textcolor>$PARAM[itemlayouttextcolor1]</textcolor>
						<label>$INFO[ListItem.Label]</label>
						<align>left</align>
					</control>
					<control type="label">
						<right>20</right>
						<top>15</top>
						<height>45</height>
						<textcolor>$PARAM[itemlayouttextcolor2]</textcolor>
						<label>$INFO[ListItem.Label2]</label>
						<align>right</align>
					</control>
					<control type="image">
						<left>56r</left>
						<top>18</top>
						<width>36</width>
						<height>36</height>
						<aspectratio>keep</aspectratio>
						<texture>$INFO[ListItem.Icon]</texture>
					</control>
				</itemlayout>
				<focusedlayout height="52" width="$PARAM[width]">
					<control type="image">
						<left>15</left>
						<top>12</top>
						<height>51</height>
						<texture border="2">buttons/ButtonFO.png</texture>
						<visible>Control.HasFocus($PARAM[id])</visible>
					</control>
					<control type="label">
						<left>30</left>
						<top>15</top>
						<height>45</height>
						<textcolor>$PARAM[focusedtextcolor1]</textcolor>
						<label>$INFO[ListItem.Label]</label>
						<align>left</align>
						<visible>Control.HasFocus($PARAM[id])</visible>
					</control>
					<control type="label">
						<right>20</right>
						<top>15</top>
						<height>45</height>
						<textcolor>$PARAM[focusedtextcolor2]</textcolor>
						<label>$INFO[ListItem.Label2]</label>
						<align>right</align>
						<visible>Control.HasFocus($PARAM[id])</visible>
					</control>
					<control type="label">
						<left>30</left>
						<top>15</top>
						<height>45</height>
						<textcolor>$PARAM[itemlayouttextcolor1]</textcolor>
						<label>$INFO[ListItem.Label]</label>
						<align>left</align>
						<visible>!Control.HasFocus($PARAM[id])</visible>
					</control>
					<control type="label">
						<right>20</right>
						<top>15</top>
						<height>45</height>
						<textcolor>$PARAM[itemlayouttextcolor2]</textcolor>
						<label>$INFO[ListItem.Label2]</label>
						<align>right</align>
						<visible>!Control.HasFocus($PARAM[id])</visible>
					</control>
					<control type="image">
						<left>56r</left>
						<top>18</top>
						<width>36</width>
						<height>36</height>
						<aspectratio>keep</aspectratio>
						<texture>$INFO[ListItem.Icon]</texture>
					</control>
				</focusedlayout>
				<include>SkinAddonContent</include>
			</control>
		</definition>
	</include>
