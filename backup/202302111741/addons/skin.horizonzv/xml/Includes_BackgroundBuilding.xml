    <include name="DialogInfoBackgroundParam">
		<param name="centerleft">50%</param>
		<param name="centerright">50%</param>
		<definition>
			<control type="group">
				<centerleft>$PARAM[centerleft]</centerleft>
				<centertop>$PARAM[centerright]</centertop>
				<width>$PARAM[width]</width>
				<height>$PARAM[height]</height>
				<include>openclosevisible</include>
				<control type="image">
					<width>$PARAM[width]</width>
					<height>$PARAM[height]</height>
					<texture>Underlay2.png</texture>
				</control>
				<control type="image">
					<width>$PARAM[width]</width>
					<height>60</height>
					<colordiffuse>$VAR[BackgroundOverlayColorDialog]</colordiffuse>
					<texture>WhiteBack.png</texture>
				</control>
				<control type="image">
					<top>$PARAM[bottomimage]</top>
					<width>$PARAM[width]</width>
					<height>60</height>
					<colordiffuse>$VAR[BackgroundOverlayColorDialog]</colordiffuse>
					<texture>WhiteBack.png</texture>
				</control>
				<control type="image">
					<width>$PARAM[width]</width>
					<height>$PARAM[height]</height>
					<texture border="5">$VAR[SelectBack2]</texture>
				</control>
				<control type="label" id="$PARAM[id]">
					<description>header label</description>
					<top>8</top>
					<width>$PARAM[width]</width>
					<height>45</height>
					<font>impacted26</font>
					<label>$PARAM[HeaderLabel]</label>
					<align>center</align>
					<aligny>center</aligny>
				</control>
				<control type="image">
					<description>mirror bottom image</description>
					<top>$PARAM[height]</top>
					<width>$PARAM[width]</width>
					<height>82</height>
					<colordiffuse>menudiffuse</colordiffuse>
					<texture background="true" flipy="true" border="4">Shadow.png</texture>
				</control>
				<control type="image">
					<top>$PARAM[top]</top>
					<width>$PARAM[width]</width>
					<height>1</height>
					<colordiffuse>grey</colordiffuse>
					<texture>Separator2.png</texture>
				</control>
				<control type="image">
					<top>60</top>
					<width>$PARAM[width]</width>
					<height>1</height>
					<colordiffuse>grey</colordiffuse>
					<texture>Separator2.png</texture>
				</control>
				<control type="image">
					<top>$PARAM[bottomimage]</top>
					<width>$PARAM[width]</width>
					<height>1</height>
					<colordiffuse>grey</colordiffuse>
