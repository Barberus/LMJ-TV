msgctxt "#301254"
msgid "[COLOR=focused][UPPERCASE][B]First Start[/B][/UPPERCASE][/COLOR][CR][CR]Welcome.[CR]These are the possibilities of navigation in the main menu.[CR][COLOR=grey1]"Skin widgets" are only visible, if the widgets are enabled in the settings.[/COLOR]"
msgstr "[COLOR=focused][UPPERCASE][B]Erster Start[/B][/UPPERCASE][/COLOR][CR][CR]Willkommen.[CR]Hier siehst du die Navigationsmöglichkeiten im Hauptmenu.[CR][COLOR=grey1]"SkinWidgets" sind nur sichtbar, wenn die Widgets in den Einstellungen aktiviert wurden.[/COLOR]"

msgctxt "#301256"
msgid "SkinWidgets .2[CR]Submenu .1"
msgstr "SkinWidgets .2[CR]Untermenü .1"

msgctxt "#301258"
msgid "Favorites .1[CR]Shutdown .2[CR]Settings .3"
msgstr "Favoriten .1[CR]Shutdown .2[CR]Einstellungen .3"

msgctxt "#301260"
msgid "In progress #2 - #10"
msgstr "Im Gange #2 - #10"

msgctxt "#301262"
msgid "[COLOR=focused][UPPERCASE][B]First start[/B][/UPPERCASE][/COLOR][CR][CR]Hide on the sides are settings panele.[CR][COLOR=grey1]Navigate left or right to open it.[CR]In horizontal views must be navigated up or down.[/COLOR]"
msgstr "[COLOR=focused][UPPERCASE][B]Erster Start[/B][/UPPERCASE][/COLOR][CR][CR]An den Seiten verbergen sich Einstellungspanele.[CR][COLOR=grey1]Navigiere nach links oder rechts um sie zu öffnen.[CR]Bei horizontalen Ansichten muss nach oben bzw. unten navigiert werden.[/COLOR]"

msgctxt "#301264"
msgid "Kodi Settings for each section.[CR](e.g. sorting)"
msgstr "Kodi Einstellungen für die jeweilige Section.[CR](z.B. Sortierung)"

msgctxt "#301266"
msgid "Settings for current view.[CR](e.g. Cover dim)"
msgstr "Einstellungen für aktuelle Ansicht.[CR](z.B. Cover dimmen)"

